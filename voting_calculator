#include <iostream>
using namespace std;
int main()
{
    int age;
    std::cout << "enter your age: " << std::endl;
    std::cin >> age;
    if(age >= 18)
    std::cout << "Your are eligible for voting " << std::endl;
    else
    {
    std::cout << "Your are not eligible for voting . You can vote after "<<18 - age << " years!" << std::endl;   
    }
}
